@using static DatabaseAccess.User.UserDb;
@model List<User>


<h1>Administrér brugere</h1>
<p class="page-description">Her kan du se information om alle brugere i systemet</p>
<div>

</div>

<table class="user-edit" cellspacing="0" cellpadding="0">
    <tr> 
        <th>Navn</th>
        <th>E-mail</th>
        <th>Institution</th>
        <th>Studieretning</th>
        
    </tr>

    @foreach(User user in Model) {
        <tr>
            <td>@user.FullName</td>
            <td>@user.Email</td>
            <td>Odense</td>
            <td>Datamatiker</td>
        </tr>
    }
</table>

